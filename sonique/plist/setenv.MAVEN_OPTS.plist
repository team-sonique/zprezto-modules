<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>setenv.MAVEN_OPTS</string>
        <key>ProgramArguments</key>
        <array>
            <string>/bin/launchctl</string>
            <string>setenv</string>
            <string>MAVEN_OPTS</string>
