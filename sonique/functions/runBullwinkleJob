#function runBullwinkleJob {
    local job_name=$1
    local bullwinkle_version=${2#dev-}
    local image_name


    if [ -z ${bullwinkle_version} ]; then
        bullwinkle_version=`_get_latest_version bullwinkle dev`
    fi

    if [[ 'DEV-SNAPSHOT' == ${bullwinkle_version} ]]; then
      image_name="bullwinkle-${job_name}:DEV-SNAPSHOT"
    else
      image_name="repo.sns.sky.com:8085/ukiss/bullwinkle-job:${bullwinkle_version%-*}"
    fi

    echo "Running Bullwinkle ${job_name} version $bullwinkle_version"
    (set -x; docker run --rm --name bullwinkle-${job_name} --net=sonique-network --net-alias=bullwinkle-${job_name} -v /tmp:/app/tuk -v /data/apps/bullwinkle/dev-${bullwinkle_version}/config:/app/config -e TZ=Europe/London ${image_name} )
#}
